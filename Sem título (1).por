programa {
  funcao inicio() {
    cadeia nome,email,endereco
    inteiro idade,cpf,numero

    escreva ("escreva eu nome:")
      leia(nome)
    escreva ("escreva sua idade:")
      leia (idade)
    escreva ("escreva seu cpf:")
      leia (cpf)
    escreva ("escreva seu email:")
      leia (email)
    escreva ("escreva seu numero de telefone")
      leia (numero)
    escreva ("escreva seu endereco")
      leia (endereco)
    escreva ("seu cadastro foi finalizado")
  }
}